{
  "ignorePatterns": ["dist"],
  "env": { "browser": true, "es6": true },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-console": [2, { "allow": ["warn", "error", "info"] }],
    "no-prototype-builtins": [0]
  }
}
